<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleBgImageView">


        <attr name="draw_type" format="enum">
            <enum name="share" value="0" />
            <enum name="edit" value="1" />
            <enum name="delete" value="2" />
            <enum name="more" value="3" />

        </attr>

    </declare-styleable>

    <declare-styleable name="DragView">


        <attr name="rotate_degree" format="integer"></attr>

    </declare-styleable>


    <declare-styleable name="TagCloudView">
        <attr name="autoScrollMode">
            <enum name="disable" value="0"/>
            <enum name="decelerate" value="1"/>
            <enum name="uniform" value="2"/>
        </attr>

        <attr name="radiusPercent" format="float"/>
        <attr name="scrollSpeed" format="float"/>
        <attr name="lightColor" format="color"/>
        <attr name="darkColor" format="color"/>
    </declare-styleable>
</resources>